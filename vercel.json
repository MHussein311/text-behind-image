{
    "version": 2,
    "functions": {
        "app.py": {
          "includeFiles": ["app.py", "requirements.txt", "templates/**"]
        }
      },
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }
  